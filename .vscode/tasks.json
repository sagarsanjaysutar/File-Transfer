{
  "tasks": [
    {
      "label": "Run Application",
      "type": "shell",
      "command": "sh",
      "args": ["-c", "./execute.sh"],
      "group": {
        "kind": "build",
        "isDefault": true
      }
    },
    {
      "label": "Build Application",
      "type": "shell",
      "command": "sh",
      "args": ["-c", "./execute.sh build"],
      "group": {
        "kind": "build",
        "isDefault": false
      }
    }
  ],
  "version": "2.0.0"
}
