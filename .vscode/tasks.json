{
  "tasks": [
    {
      "type": "cppbuild",
      "label": "QT C++ Build",
      "command": "/usr/bin/g++",
      "args": [
        "-fdiagnostics-color=always",
        "-g",
        "${file}",
        "-o",
        "${workspaceFolder}/.output/${fileBasenameNoExtension}.o",
        "src/*.cpp",
        "-I",
        "inc/",
        "-I",
        "/home/ssutar/Deep-Trekker/QT/5.11.3/gcc_64/include/",
        "-I",
        "/home/ssutar/Deep-Trekker/QT/5.11.3/gcc_64/include/QtCore",
        "-L",
        "/home/ssutar/Deep-Trekker/QT/5.11.3/gcc_64/lib/",
        "-l",
        "Qt5Core",
        "-I",
        "/home/ssutar/Deep-Trekker/QT/5.11.3/gcc_64/include/QtNetwork",
        "-l",
        "Qt5Network",
        "-fPIC",
        "-std=c++11"     
      ],
      "options": {
        "cwd": "${fileDirname}"
      },
      "problemMatcher": ["$gcc"],
      "group": "build",
      "detail": "Task generated by Debugger."
    },
    {
      "label": "QT C++ Run",
      "type": "shell",
      "command": "${workspaceFolder}/.output/${fileBasenameNoExtension}.o",
      "options": {
        "cwd": "${fileDirname}"
      },
      "group": {
        "kind": "build",
        "isDefa -lult": true
      },
      "presentation": {
        "reveal": "always",
        "panel": "new"
      },     
      "problemMatcher": []
    },
    
    {
      "label": "QT Build and Run",
      "dependsOrder": "sequence",
      "dependsOn": ["QT C++ Build", "QT C++ Run"],
      "group": {
        "kind": "build"        
      }
    }
  ],
  "version": "2.0.0"
}
